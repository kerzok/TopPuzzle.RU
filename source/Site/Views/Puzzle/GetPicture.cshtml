@model Toppuzzle.Site.Models.PictureViewModel

@{
    Layout = null;
}
<canvas id="editor" height="@Model.Height" width="@Model.Width"></canvas>
<div id="image-parts">
    @foreach (var i in Model.RandomList)
{
    <img src="data:image/png;base64,@Model.Parts.ElementAt(i)" alt=""  index="@i" width="@(Model.Width / 6)" height="@(Model.Height / 6)" style="display: inline-block; margin: 5px; border:black; border-style: double"/>
}
</div>
<img src="@Url.Content(Model.Source)" alt="" width="@(Model.Width / 5)" height="@(Model.Height / 5)" style="display: inline-block"/>